{
	"name": "My Password Guru",
	"description": "A password locker that generates your password based on your personalization and interest.",
	"version": "alpha-1.5",
	"manifest_version": 3,
	"permissions": [
		"storage"
	],
	"action": {
		"default_popup": "index.html",
		"default_title": "My Password Guru"
	},
	"icons": [
        {
            "src": "/android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "/android-chrome-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        }
    ]
}